package run;

import xml.BDP;
import xml.BDPC;

public class Run {
	public static void main (String args[]){
		
		try{
			BDPC bdpc = xml.XmlOps.readXmlInput();
			bdpc.sort();

			System.out.println("Die nächsten " + args[0] + " Geburtstage sind:\n");
			for(int i = 0; i < Integer.parseInt(args[0]); i++){
				System.out.println(bdpc.getBd_list().get(i));
			}


		}catch(Exception e){
			e.printStackTrace();
		}
	}
}